  "title": "PERS CD-VETERAN PREFERENCE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Personnel Code. Provides that a veteran is qualified for a preference of 5 points if the veteran served in active service for a total consecutive or non-consecutive period of at least 3 years, including as a member of the Illinois National Guard in service to the State of Illinois and including for training or other purposes. Adds a definition."
    }
  ],

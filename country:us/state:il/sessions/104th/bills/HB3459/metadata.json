  "identifier": "HB3459",
  "title": "EMPLOYMENT-32 HOUR WORK WEEK",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Minimum Wage Law. Provides that no employer shall employ any of his employees for a workweek of more than 32 hours (currently, 40 hours) unless such employee receives compensation for his employment in excess of the hours above specified at a rate not less than 1 1/2 times the regular rate at which he is employed. Makes a conforming change."
    }
  ],

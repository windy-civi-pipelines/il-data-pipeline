  "identifier": "SB1682",
  "title": "COUNTY BOARD MEMBER LIMITATION",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Public Officer Prohibited Activities Act. In provisions concerning offices a member of a county board may hold during the member's term of office, provides that a member of the county board may serve as a member of an intergovernmental joint self-insurance pool board during the county board member's term of office. Effective immediately."
    }
  ],

  "identifier": "HB3443",
  "title": "OMA-COUNCIL DEV DISAB",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Open Meetings Act. Provides that the requirement that a quorum be physically present at the location of an open meeting shall not apply to the Illinois Council on Developmental Disabilities. Amends the Illinois Council on Developmental Disabilities Law. Provides that a member of the Council may request permission of the chairperson to attend a meeting by video or audio conference, and that request shall be granted if the member is prevented from physically attending due to personal illness or disability or if the member is required to provide care to a family member who has a disability. Effective immediately."
    }
  ],

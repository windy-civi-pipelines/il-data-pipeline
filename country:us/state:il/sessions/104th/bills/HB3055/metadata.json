  "identifier": "HB3055",
  "title": "HIGHER ED-ASSIST-NONCITIZEN",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Higher Education Student Assistance Act. Provides that, notwithstanding any other law, a student who otherwise qualifies for any grant or scholarship offered under this Act, by any other State law, or by a unit of local government shall not be denied the award of the grant or scholarship based solely on the student's status as a noncitizen. Effective immediately."
    }
  ],

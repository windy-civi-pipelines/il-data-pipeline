{
  "legislative_session": "104th",
  "identifier": "HB42",
  "title": "LIBRARIES-NONRESIDENTS-FEES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Local Library Act and the Public Library District Act of 1991. Authorizes costs paid by persons residing outside of the relevant library jurisdiction to be paid quarterly or biannually if permitted by board regulations. Effective immediately."
    }
  ],

  "legislative_session": "104th",
  "identifier": "HR507",
  "title": "AUTISM-SEC. KENNEDY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Condemns the dangerous, false, and demeaning statements of Robert F. Kennedy Jr. and reaffirms Illinois' commitment to defending science, truth, and respect for all individuals with developmental differences. Urges Robert F. Kennedy Jr. to immediately resign as Secretary of Health and Human Services. If Secretary Kennedy fails to resign, urges President Trump to immediately remove him as Secretary of Health and Human Services."
    }
  ],

  "legislative_session": "104th",
  "identifier": "HR505",
  "title": "FREE PRESS-TRUMP ADMIN.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Affirms support for the freedom of the press and the freedom of speech as outlined in the First Amendment to the United States Constitution. Recognizes the Trump Administration's efforts to exert editorial control over media companies by using the official powers of the government to punish dissent and reward acquiescence as textbook corruption. Condemns President Trump's efforts to restrict the right to free speech as attacks on democracy itself."
    }
  ],

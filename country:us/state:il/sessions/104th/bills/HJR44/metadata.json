{
  "legislative_session": "104th",
  "identifier": "HJR44",
  "title": "DHS-DELAY CUTS-DSP SERVICE HRS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Urges the Illinois Department of Human Services and the Pritzker Administration to delay cuts to Direct Service Professional (DSP) service hours set to begin January 1,2026, until such time as the State is prepared to implement the \"Zero Hour\" Staffing Model, to protect the services depended on by individuals with intellectual and developmental disabilities (I/DD)."
    }
  ],

  "identifier": "SB2666",
  "title": "VIDEO GAMING-PROHIBITION",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Video Gaming Act. Provides that a municipality with a population that is greater than 2,000,000 may not enforce any local ordinance in effect on the effective date of the amendatory Act prohibiting the operation of video gaming terminals within the corporate limits of the municipality. Effective immediately."
    }
  ],

  "title": "NEWBORN SCREEN-BLOOD DISORDER",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Newborn Metabolic Screening Act. Provides that, subject to appropriation, the Department of Public Health shall provide all newborns with screening tests for rare blood disorders, as determined by the Department. Requires the Department to adopt rules."
    }
  ],

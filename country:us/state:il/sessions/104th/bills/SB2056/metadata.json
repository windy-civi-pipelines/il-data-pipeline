  "title": "MOBILE TELEDENTISTRY EXAMS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Dental Practice Act. Provides that a patient of record who is provided services through a mobile dental van or portable dental unit does not need to receive an in-person physical examination from a dentist prior to receiving treatment that utilizes teledentistry if such services are limited to certain procedures."
    }
  ],

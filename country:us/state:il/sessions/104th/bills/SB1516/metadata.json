  "identifier": "SB1516",
  "title": "ID CARD-VICTIM WAIVER",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Identification Card Act. Provides that the Secretary of State shall not collect a fee for a standard Illinois Identification Card issued to a transgender person, or the immediate family of that person, moving to Illinois due to an act of persecution. Effective immediately."
    }
  ],

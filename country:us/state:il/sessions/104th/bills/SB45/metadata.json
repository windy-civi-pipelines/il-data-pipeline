  "title": "BAN DRAGGING BOVINE BY TAIL",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Humane Care for Animals Act. Provides that no person may intentionally drag or pull any bovine by its tail by any means for the purpose of entertainment, sport, practice, or contest."
    }
  ],
